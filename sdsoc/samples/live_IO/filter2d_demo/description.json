{
	"example" : "Filter 2D Simple Demo",
	"overview" : "2D convolution filter based on HLS video libraries",
	"board" : [
		"zybo_z7_20"
	],
	"os" : [
		"Linux"
	],
	"system" : {
		"dmclkid" : "0"
	},
	"compiler" : {
		"symbols" : [
			"__ARM_PCS_VFP",
			"HLS_NO_XIL_FPO_LIB",
			"WITH_SDSOC"
		],
		"options" : "-I${SYSROOT}/usr/include -I${SYSROOT}/../linux/inc/xfopencv/imgproc -I${SYSROOT}/usr/lib/glib-2.0/include -Wno-overloaded-virtual -Wno-unused-label -Wno-strict-overflow -Wno-uninitialized -Wno-unused-function -Wno-unused-variable -Wno-unknown-attributes -Wno-unused-local-typedefs -Wno-sign-compare"
	},
	"linker" : {
		"libraries" : [
			"glib-2.0",
			"opencv_core",
			"opencv_imgproc"
		],
		"options" : "--sysroot=${SYSROOT} -L=/lib -L=/usr/lib -Wl,-rpath-link=${SYSROOT}/lib,-rpath-link=${SYSROOT}/usr/lib"
	}
}
